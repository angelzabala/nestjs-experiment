{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "NestJS Debug",
        "runtimeArgs": [
          "run-script",
          "start:debug"
        ],
        "runtimeExecutable": "npm",
        "restart": true,
        "console": "integratedTerminal",
        "internalConsoleOptions": "never",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "port": 9229
      },
      {
        "type": "node",
        "request": "attach",
        "name": "Attach to NestJS Debug",
        "port": 9229,
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
  }
  